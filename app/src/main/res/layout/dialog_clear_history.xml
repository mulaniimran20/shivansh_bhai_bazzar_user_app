<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.view.menu.ListMenuItemView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_15sdp"
    android:clickable="true"
    android:focusable="true"
    android:orientation="vertical"
   android:layout_gravity="center">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_15sdp"
        android:clickable="true"
        android:focusable="true"
        android:orientation="vertical"
        app:cardCornerRadius="@dimen/_5sdp"
        app:cardElevation="@dimen/_5sdp">


        <LinearLayout
            android:id="@+id/menu_cvProfile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_5sdp"
            android:orientation="vertical">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <com.aarfaatechnovision.kjl.shivanshbhajibazar.customecomponent.CustomTextView
                    style="@style/ganaral_header_style"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/_5sdp"
                    android:text="@string/clear_history"
                    app:font_name="NunitoBold.ttf" />


                <ImageView
                    android:id="@+id/dailog_history_ivClose"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/_5sdp"
                    android:src="@drawable/ic_close" />

            </RelativeLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">


                <com.aarfaatechnovision.kjl.shivanshbhajibazar.customecomponent.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/_3sdp"
                    android:text="@string/this_includes"
                    android:textAllCaps="true"
                    android:textColor="@color/signup_tab_color"
                    android:textSize="@dimen/_12sdp"
                    app:font_name="NunitoBold.ttf" />

                <com.aarfaatechnovision.kjl.shivanshbhajibazar.customecomponent.CustomTextView
                    android:id="@+id/dailog_history_tvSearchHistory"
                    style="@style/ganaral_lable_style"
                    android:paddingStart="@dimen/_3sdp"
                    android:text="@string/search_history"
                    android:layout_marginTop="@dimen/_5sdp"
                    app:font_name="NunitoBold.ttf" />

                <com.aarfaatechnovision.kjl.shivanshbhajibazar.customecomponent.CustomTextView
                    android:id="@+id/dailog_history_tvOrderHistory"
                    style="@style/ganaral_lable_style"
                    android:paddingStart="@dimen/_3sdp"
                    android:text="@string/order_history"
                    android:layout_marginTop="@dimen/_5sdp"
                    app:font_name="NunitoBold.ttf" />




            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_10sdp">

                <com.aarfaatechnovision.kjl.shivanshbhajibazar.customecomponent.CustomTextView
                    android:id="@+id/dailog_history_tvCancel"
                    style="@style/ganaral_btn_save_style"
                    android:layout_toStartOf="@+id/dailog_history_tvClearAll"
                    android:padding="@dimen/_5sdp"
                    android:text="@string/cancel"
                    app:font_name="NunitoSemiBold.ttf" />


                <com.aarfaatechnovision.kjl.shivanshbhajibazar.customecomponent.CustomTextView
                    android:id="@+id/dailog_history_tvClearAll"
                    style="@style/ganaral_btn_save_style"
                    android:layout_alignParentEnd="true"
                    android:padding="@dimen/_5sdp"
                    android:text="@string/clear_all"
                    app:font_name="NunitoExtraBold.ttf" />


            </RelativeLayout>


        </LinearLayout>


    </android.support.v7.widget.CardView>


</android.support.v7.view.menu.ListMenuItemView>